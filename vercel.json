{
  "buildCommand": "cd backend && pip install -r requirements.txt",
  "outputDirectory": "backend",
  "functions": {
    "api/index.py": {
      "runtime": "python3.9",
      "includeFiles": "backend/**"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    }
  ]
}
